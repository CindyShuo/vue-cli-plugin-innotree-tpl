{
  "useConfigFiles": true,
  "cssPreprocessor": "stylus",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save", "commit"]
    },
    "vue-cli-plugin-innotree-tpl": {
      "replaceTemplates": true
    }
  },
  "configs": {
    "vue": {
      "devServer": {
        "proxy": {
          "/api": {
            "target": "",
            "changeOrigin": true,
            "pathRewrite": {
              "^/api": ""
            }
          }
        }
      }
    }
  }
}
